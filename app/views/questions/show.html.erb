<p id="notice"><%= notice %></p>


<p>
  <strong>User:</strong>
  <%= @question.user_id %>
</p>

<p>
  <strong>質問</strong>
  <br>
  <%= safe_join(@question.question.split("\n"),tag(:br)) %>
</p>
<% @answers.each do |answer| %>
   <p>
    <b>回答内容
      <%= answer.answer %>
   </p>
<% end %>

<%= link_to 'Edit', edit_question_path(@question) %> |
<%= link_to 'Back', questions_path %>
